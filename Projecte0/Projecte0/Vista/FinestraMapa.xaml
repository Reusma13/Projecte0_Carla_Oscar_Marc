        <Button Name="ButtonTaula3" Content="Taula 3" HorizontalAlignment="Left" Margin="454,283,0,0" VerticalAlignment="Top" Height="19" Width="68" Click="ButtonTaula3_Click" Background="DarkRed" Foreground="White"/>
